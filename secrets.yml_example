# Secrets Configuration
# Add your API keys here - DO NOT commit this file to version control!

# Access control
# If allowed_users is non-empty, only these user identifiers are permitted.
# For Telegram, this is typically the Telegram username (without '@').
allowed_users:
  - usersplintermaster

# AWS Bedrock Configuration
aws:
  region: us-east-1
  # AWS credentials are typically loaded from environment or ~/.aws/credentials
  access_key_id: XXXX
  secret_access_key: XXXX

# Bedrock API Key (for cross-region inference)
bedrock:
  api_key: XXXX

google:
  api_key: "XXX"

# OpenAI Configuration
openai:
  api_key: XXXX

# Telegram Bot Token
telegram:
  bot_token: XXXX

# Skills secrets
skills:
  GEMINI_API_KEY: XXXX
  TAVILY_API_KEY: XXXX
